@prefix ex: <http://example.org/uni#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

### CLASS DEFINITIONS – a stands for rdf:type in Turtle
ex:Course a rdfs:Class .
ex:Skill a rdfs:Class .
ex:ResearchPaper a rdfs:Class .
ex:CareerPath a rdfs:Class .
ex:Professor a rdfs:Class .


### PROPERTY DEFINITIONS
### COURSE CLASS

ex:courseTitle a rdf:Property ;
	rdfs:domain ex:Course ;
	rdfs:range xsd:string .

ex:creditPoints a rdf:Property ;
	rdfs:domain ex:Course ;
	rdfs:range xsd:integer .

ex:teachesSkill a rdf:Property ;
	rdfs:domain ex:Course ;
	rdfs:range ex:Skill .

ex:prerequisite a rdf:Property ;
	rdfs:domain ex:Course ;
	rdfs:range ex:Course .

ex:taughtBy a rdf:Property ;
	rdfs:domain ex:Course ;
	rdfs:range ex:Professor .

ex:isCoreCourse a rdf:Property ;
    rdfs:domain ex:Course ;
    rdfs:range xsd:boolean .

### SKILL CLASS
ex:label a rdf:Property ;
	rdfs:domain ex:Course ;
	rdfs:range xsd:string .

### ResearchPaper CLASS
ex:paperTitle a rdf:Property ;
	rdfs:domain ex:ResearchPaper ;
	rdfs:range xsd:string .

ex:paperTitle a rdf:Property ;
	rdfs:domain ex:ResearchPaper ;
	rdfs:range xsd:string .

ex:paperTitle a rdf:Property ;
	rdfs:domain ex:ResearchPaper ;
	rdfs:range xsd:integer .

ex:authoredBy a rdf:Property ;
	rdfs:domain ex:ResearchPaper ;
	rdfs:range ex:Professor .

ex:researchDomain a rdf:Property ;
	rdfs:domain ex:ResearchPaper ;
	rdfs:range ex:Course .

### CareerPath CLASS
ex:careerTitle a rdf:Property ;
	rdfs:domain ex:CareerPath ;
	rdfs:range xsd:string .

ex:requiresSkill a rdf:Property ;
	rdfs:domain ex:CareerPath ;
	rdfs:range ex:Skill .

### Professor CLASS

ex:name a rdf:Property ;
	rdfs:domain ex:Professor ;
	rdfs:range xsd:string .

